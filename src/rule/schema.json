{
  "$schema": "http://json-schema.org/schema",
  "id": "SchematicsGitWebhooksRule",
  "title": "Hygie Rule Options Schema",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "description": "The name of the rule.",
      "$default": {
        "$source": "argv",
        "index": 0
      },
      "x-prompt": "What name would you like to use for the rule?"
    }
  },
  "required": ["name"]
}

{
  "$schema": "http://json-schema.org/schema",
  "id": "SchematicsGitWebhooksRunnable",
  "title": "Hygie Runnable Options Schema",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "description": "The name of the runnable.",
      "$default": {
        "$source": "argv",
        "index": 0
      },
      "x-prompt": "What name would you like to use for the runnable?"
    }
  },
  "required": ["name"]
}
